{
  "type": "module",
  "name": "@rvohealth/psychic",
  "description": "Typescript web framework",
  "version": "0.24.0",
  "author": "RVOHealth",
  "repository": "https://github.com/rvohealth/psychic.git",
  "license": "MIT",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "client": "yarn --cwd=./client start",
    "publish": "yarn npm publish",
    "psy": "yarn psyts",
    "psyjs": "node ./dist/test-app/src/cli/index.js",
    "psyts": "node --experimental-specifier-resolution=node --trace-warnings --loader ts-node/esm -r ts-node/register/transpile-only ./test-app/src/cli/index.ts",
    "psycore": "PSYCHIC_CORE_DEVELOPMENT=1 yarn psyts",
    "gpsycore": "PSYCHIC_CORE_DEVELOPMENT=1 npx ts-node --transpile-only ./global-cli/main.ts",
    "build": "echo \"building cjs...\" && rm -rf dist && npx tsc -p ./tsconfig.cjs.build.json && echo \"building esm...\" && npx tsc -p ./tsconfig.esm.build.json",
    "build:core": "rm -rf dist && echo \"building psychic core...\" && npx tsc -p ./tsconfig.build-core.json",
    "sync": "NODE_ENV=development yarn --cwd=node_modules/@rvohealth/dream build-boot && NODE_ENV=development yarn --cwd=node_modules/@rvohealth/dream build",
    "syncb": "NODE_ENV=development yarn --cwd=node_modules/@rvohealth/dream build-boot && NODE_ENV=development yarn dream sync:config-cache && yarn --cwd=node_modules/@rvohealth/dream build && yarn build",
    "dev": "PSYCHIC_CORE_DEVELOPMENT=1 NODE_ENV=development ts-node ./test-app/main.ts",
    "console": "PSYCHIC_CORE_DEVELOPMENT=1 npx ts-node --transpile-only --project ./tsconfig.json ./test-app/src/conf/repl.ts",
    "uspec": "PSYCHIC_CORE_DEVELOPMENT=1 NODE_ENV=test vitest --no-file-parallelism --maxWorkers=1 --minWorkers=1 --maxConcurrency=1 --config ./spec/unit/vite.config.ts",
    "fspec": "PSYCHIC_CORE_DEVELOPMENT=1 NODE_ENV=test vitest run --config=./spec/features/vite.config.ts",
    "spec-server": "FEATURE_SPEC_RUN=1 SPEC_SERVER_PORT=7770 PORT=7770 npx ts-node --transpile-only ./test-app/spec-server.ts",
    "format": "yarn run prettier . --write",
    "lint": "yarn run eslint --no-warn-ignored \"src/**/*.ts\" \"spec/**/*.ts\" \"test-app/**/*.ts\" && yarn run prettier . --check",
    "prepack": "yarn build"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/lodash.groupby": "^4.6.9",
    "colorette": "^2.0.20",
    "commander": "^12.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express-openapi-validator": "^5.4.3",
    "iso-datestring-validator": "^2.2.2",
    "lodash-es": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "pluralize-esm": "^9.0.5"
  },
  "peerDependencies": {
    "@rvohealth/dream": "*",
    "@types/express": "*",
    "commander": "*",
    "express": "*"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@jest-mock/express": "^3.0.0",
    "@playwright/test": "^1.51.0",
    "@rvohealth/dream": "=0.29.0-fgbeta-20",
    "@rvohealth/dream-spec-helpers": "=0.1.0-fgbeta-15",
    "@rvohealth/psychic-spec-helpers": "=0.2.0-fgbeta-1",
    "@testing-library/dom": "^10.4.0",
    "@types/express": "^4.17.21",
    "@types/lodash-es": "^4",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/lodash.sortby": "^4.7.9",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.5.1",
    "@types/pg": "^8.11.8",
    "@types/supertest": "^6.0.2",
    "@vitest/browser": "^3.0.8",
    "eslint": "^9.19.0",
    "express": "^4.21.1",
    "jest-dev-server": "^11.0.0",
    "jsdom": "^26.0.0",
    "kysely": "^0.27.4",
    "kysely-codegen": "~0.17.0",
    "lodash.sortby": "^4.7.0",
    "luxon": "^3.5.0",
    "luxon-jest-matchers": "^0.1.14",
    "pg": "^8.12.0",
    "prettier": "^3.3.3",
    "puppeteer": "^24.4.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typedoc": "^0.26.6",
    "typescript": "^5.5.4",
    "typescript-eslint": "=7.18.0",
    "vitest": "^3.0.8",
    "vitest-puppeteer": "^11.0.3",
    "webdriverio": "^9.11.0",
    "winston": "^3.14.2"
  },
  "packageManager": "yarn@4.4.1"
}
