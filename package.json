{
  "name": "psychic",
  "version": "0.0.1",
  "description": "MVC web framework that knows what you're thinking. written in nodejs",
  "private": true,
  "main": ".dist/index.js",
  "bin": {
    "psy": ".dist/bin/psy.js"
  },
  "scripts": {
    "test": "npm run buildspec &&  CORE_TEST=true node --experimental-vm-modules node_modules/.bin/jest --config ./jest.config.json --runInBand --detectOpenHandles",
    "testquickly": " CORE_TEST=true node --experimental-vm-modules node_modules/.bin/jest --config ./jest.config.json --runInBand --detectOpenHandles",
    "copyhelpers": " ./node_modules/.bin/babel src/helpers -d .dist/make/helpers --copy-files --quiet",
    "copymake": "npm run copyhelpers &&  ./node_modules/.bin/babel make -d .dist/make --copy-files --quiet",
    "build": "npm run copymake &&  ./node_modules/.bin/babel src -d .dist --copy-files --quiet",
    "buildspec": "npm run copymake &&  node ./.dist/make/for-core-specs.js &&  ./node_modules/.bin/babel src -d .dist --copy-files --quiet &&  ./node_modules/.bin/babel spec/support/testapp -d .dist/testapp --copy-files --quiet",
    "prepare": "npm run build --scripts-prepend-node-path --silent &&  node ./.dist/make/post-install.js",
    "psy": "npm run build --scripts-prepend-node-path --silent && clear && node ./.dist/cli/run.js"
  },
  "engines": {
    "node": ">=16.8.0",
    "yarn": "*"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^3.0.0",
    "husky": "^7.0.2",
    "jest": "27.3.0",
    "jest-date": "^1.1.4",
    "jest-plugin-context": "^2.9.0",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.21.0",
    "@aws-sdk/s3-request-presigner": "^3.21.0",
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.0",
    "@babel/eslint-parser": "^7.13.0",
    "@babel/node": "^7.13.0",
    "@babel/plugin-proposal-async-do-expressions": "^7.13.0",
    "@babel/plugin-proposal-do-expressions": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.0",
    "@babel/preset-env": "^7.13.0",
    "@babel/preset-react": "^7.13.0",
    "axios": "^0.21.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "bull": "^3.29.3",
    "chalk": "^4.1.1",
    "concurrently": "^6.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "http-terminator": "^3.0.0",
    "ioredis": "^4.28.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.13",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.2",
    "npm": "^8.0.0",
    "ora": "^5.4.0",
    "pg": "^8.6.0",
    "pg-format": "^1.0.4",
    "pluralize": "^8.0.0",
    "redis": "^3.1.2",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2",
    "twilio": "^3.64.0",
    "uuid": "^8.3.2",
    "ws": "^7.4.6"
  }
}
