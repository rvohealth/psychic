{
  "name": "psychic",
  "version": "0.0.1",
  "description": "MVC web framework that knows what you're thinking. written in nodejs",
  "private": true,
  "main": ".dist/index.js",
  "scripts": {
    "test": "npm run buildspec && NODE_PATH=. CORE_TEST=true node --experimental-vm-modules ./node_modules/.bin/jest --config ./jest.config.json --runInBand --detectOpenHandles",
    "copyhelpers": "NODE_PATH=. ./node_modules/.bin/babel src/helpers -d .dist/make/helpers --copy-files --quiet",
    "copymake": "npm run copyhelpers && NODE_PATH=. ./node_modules/.bin/babel make -d .dist/make --copy-files --quiet",
    "build": "npm run copymake && NODE_PATH=. ./node_modules/.bin/babel src -d .dist --copy-files --quiet",
    "buildspec": "npm run copymake && NODE_PATH=. node ./.dist/make/for-core-specs.js && NODE_PATH=. ./node_modules/.bin/babel src -d .dist --copy-files --quiet && NODE_PATH=. ./node_modules/.bin/babel spec/support/testapp -d .dist/testapp --copy-files --quiet",
    "prepare": "npm run build --scripts-prepend-node-path --silent",
    "psy": "npm run build --scripts-prepend-node-path --silent && clear && NODE_PATH=./ node ./.dist/cli/run.js"
  },
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "apidoc": "^0.17.5",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "babel-plugin-import-directory": "^1.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.2.0",
    "factory-bot": "^6.3.1",
    "factory-girl": "^5.0.4",
    "fishery": "^1.4.0",
    "husky": "^3.0.7",
    "jest": "26.6.0",
    "jest-date": "^1.1.4",
    "jest-plugin-context": "^2.9.0",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.7",
    "nyc": "^14.1.1",
    "opn-cli": "^5.0.0",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.0.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.21.0",
    "@aws-sdk/s3-request-presigner": "^3.21.0",
    "@babel/preset-react": "^7.14.5",
    "assignment": "^2.2.1",
    "axios": "^0.21.1",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-wildcard": "^7.0.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "bree": "^6.2.0",
    "bull": "^3.29.2",
    "chalk": "^4.1.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^10.0.0",
    "dotenv-safe": "^6.0.0",
    "email-templates": "^6.0.3",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-validation": "^1.0.2",
    "fs-extra": "^10.0.0",
