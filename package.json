    "mongoose": "^5.2.17",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.2",
    "npm": "^7.14.0",
    "ora": "^5.4.0",
    "pg": "^8.6.0",
    "pg-format": "^1.0.4",
    "pluralize": "^8.0.0",
