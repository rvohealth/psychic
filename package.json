{
  "name": "@rvohealth/psychic",
  "description": "Typescript web framework",
  "version": "0.7.6",
  "main": "dist/src/index.js",
  "author": "RVOHealth",
  "repository": "https://github.com/rvohealth/psychic.git",
  "license": "MIT",
  "private": false,
  "files": [
    "tsconfig.build.json",
    "tsconfig.global-cli.json",
    "tsconfig.json",
    "boilerplate/**/*",
    ".global-cli-dist/**/*",
    "src/**/*",
    "dist/**/*"
  ],
  "bin": {
    "psy": "./.global-cli-dist/main.js"
  },
  "scripts": {
    "psy": "yarn psyts",
    "psyjs": "node ./dist/test-app/cli/index.js",
    "psyts": "npx ts-node --transpile-only -r ./src/helpers/loadEnv.ts test-app/cli/index.ts",
    "psycore": "PSYCHIC_CORE_DEVELOPMENT=1 yarn psyts",
    "gpsycore": "npx ts-node --transpile-only ./global-cli/main.ts",
    "build": "rm -rf dist && echo \"building psychic app...\" && npx tsc -p ./tsconfig.build.json",
    "build:global-cli": "npx tsc -p ./tsconfig.global-cli.json",
    "build:core": "rm -rf dist && echo \"building psychic core...\" && npx tsc -p ./tsconfig.build-core.json",
    "sync": "NODE_ENV=development yarn --cwd=node_modules/@rvohealth/dream build-boot && NODE_ENV=development yarn --cwd=node_modules/@rvohealth/dream build",
    "syncb": "NODE_ENV=development yarn --cwd=node_modules/@rvohealth/dream build-boot && NODE_ENV=development yarn dream sync:config-cache && yarn --cwd=node_modules/@rvohealth/dream build && yarn build",
    "dev": "PSYCHIC_CORE_DEVELOPMENT=1 NODE_ENV=development ts-node ./test-app/main.ts",
    "console": "PSYCHIC_CORE_DEVELOPMENT=1 npx ts-node --transpile-only --project ./tsconfig.json ./test-app/conf/repl.ts",
    "uspec:slow": "PSYCHIC_CORE_DEVELOPMENT=1 yarn psyts sync:all --core && PSYCHIC_CORE_DEVELOPMENT=1 jest --config=./spec/unit/jest.config.js --runInBand --forceExit",
    "uspec": "PSYCHIC_CORE_DEVELOPMENT=1 jest --config=./spec/unit/jest.config.js --runInBand --forceExit",
    "fspec:slow": "PSYCHIC_CORE_DEVELOPMENT=1 yarn psyts sync:all --core && PSYCHIC_CORE_DEVELOPMENT=1 jest --config=./spec/features/jest.config.js --runInBand --forceExit",
    "fspec": "PSYCHIC_CORE_DEVELOPMENT=1 DEV_SERVER_PORT=7770 jest --config=./spec/features/jest.config.js --runInBand --forceExit",
    "spec-server": "FEATURE_SPEC_RUN=1 DEV_SERVER_PORT=7770 PORT=7770 npx ts-node --transpile-only ./test-app/spec-server.ts",
    "format": "yarn run prettier . --write",
    "lint": "yarn run eslint --no-warn-ignored \"src/**/*.ts\" \"spec/**/*.ts\" \"test-app/**/*.ts\" && yarn run prettier . --check",
    "prepack": "yarn build"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^8.2.1",
    "@socket.io/redis-emitter": "^5.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/lodash.groupby": "^4.6.9",
    "@types/pluralize": "^0.0.33",
    "bcrypt": "^5.1.1",
    "colorette": "^2.0.20",
    "commander": "^12.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express-openapi-validator": "^5.3.1",
    "iso-datestring-validator": "^2.2.2",
    "lodash.groupby": "^4.6.0",
    "openapi-typescript": "^7.1.0",
    "pluralize": "^8.0.0"
  },
  "peerDependencies": {
    "@rvohealth/dream": "^0.10.7",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "bullmq": "^5.1.11",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "redis": "^4.6.13",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@jest-mock/express": "^2.0.2",
    "@rvohealth/dream": "^0.10.7",
    "@rvohealth/dream-spec-helpers": "^0.0.2",
    "@rvohealth/psychic-spec-helpers": "^0.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jest-plugin-context": "^2.9.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.17",
    "@types/supertest": "^6.0.2",
    "@types/winston": "^2.4.4",
    "bullmq": "^5.1.11",
    "eslint": "^9.0.0",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "jest-plugin-context": "^2.9.0",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.2",
    "kysely-codegen": "^0.11.0",
    "luxon": "^3.4.4",
    "pg": "^8.11.3",
    "prettier": "^3.2.5",
    "redis": "^4.6.13",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.6.0",
    "winston": "^3.14.2"
  }
}
