{
  "name": "dreamtest",
  "version": "0.0.0",
  "main": "src/index.ts",
  "author": "avocadojesus <jgarbey@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && npx tsc",
    "dream": "yarn --cwd=node_modules/dream dream",
    "build-dev": "CORE_DEVELOPMENT=1 ts-node ./bin/build-psychic-dir.ts  && yarn build && yarn build-globals-dev",
    "build-globals-dev": "CORE_DEVELOPMENT=1 ts-node ./test-app/.psy/buildGlobals.ts",
    "copy:models": "npx ts-node ./.dream/bin/copy-models.ts",
    "db:create": "yarn --cwd=node_modules/dream dream db:create",
    "db:drop": "yarn --cwd=node_modules/dream dream db:drop",
    "db:migrate": "yarn --cwd=node_modules/dream dream db:migrate",
    "sync": "yarn --cwd=node_modules/dream dream build:all",
    "c": "yarn console",
    "console": "NODE_ENV=development npx ts-node --project ./tsconfig.json ./test-app/conf/repl.ts",
    "uspec": "yarn build-dev && CORE_DEVELOPMENT=1 jest --config=./spec/unit/jest.config.js --runInBand --forceExit",
    "fspec": "yarn build-dev && CORE_DEVELOPMENT=1 jest --config=./spec/features/jest.config.js --runInBand --forceExit",
    "spec-server": "NODE_ENV=test ts-node ./test-app/spec-server.ts",
    "postinstall": "rm -rf ./node_modules/dream/test-app && yarn --cwd=node_modules/dream dream copy:boilerplate"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^8.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/jest": "^29.5.0",
    "@types/jest-plugin-context": "^2.9.5",
    "bcrypt": "^5.1.0",
    "bullmq": "^3.10.4",
    "colorette": "^2.0.19",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dream": "https://github.com/avocadojesus/dream.git#74412cb",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "jest-plugin-context": "^2.9.0",
    "jsonwebtoken": "^9.0.0",
    "pluralize": "^8.0.0",
    "redis": "^4.6.5",
    "socket.io": "^4.6.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "supertest": "^6.3.3",
    "tslib": "^2.5.0"
  }
}
