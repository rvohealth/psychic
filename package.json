{
  "name": "dreamtest",
  "version": "0.0.0",
  "main": "src/index.ts",
  "author": "avocadojesus <jgarbey@gmail.com>",
  "license": "MIT",
  "private": true,
  "bin": {
    "psy": "npx ts-node ./bin/global-cli.ts"
  },
  "scripts": {
    "psy": "npx ts-node ./bin/cli.ts",
    "dream": "yarn --cwd=node_modules/dream dream",
    "dev": "CORE_DEVELOPMENT=1 NODE_ENV=development ts-node ./test-app/main.ts",
    "build": "rm -rf dist && npx tsc",
    "db:create": "yarn dream db:create",
    "db:drop": "yarn dream db:drop",
    "db:migrate": "yarn dream db:migrate",
    "sync-core": "yarn psy sync:all --core && yarn dream sync:all",
    "uspec": "yarn sync-core && CORE_DEVELOPMENT=1 jest --config=./spec/unit/jest.config.js --runInBand --forceExit",
    "fspec": "yarn sync-core && CORE_DEVELOPMENT=1 jest --config=./spec/features/jest.config.js --runInBand --forceExit"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^8.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/jest": "^29.5.0",
    "@types/jest-plugin-context": "^2.9.5",
    "@types/jsonwebtoken": "^9.0.1",
    "bcrypt": "^5.1.0",
    "bullmq": "^3.10.4",
    "colorette": "^2.0.19",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dream": "https://github.com/avocadojesus/dream.git#4643047",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "jest-plugin-context": "^2.9.0",
    "jsonwebtoken": "^9.0.0",
    "pluralize": "^8.0.0",
    "redis": "^4.6.5",
    "socket.io": "^4.6.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "supertest": "^6.3.3",
    "tslib": "^2.5.0"
  }
}
