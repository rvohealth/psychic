{
  "type": "module",
  "name": "@rvoh/psychic",
  "description": "Typescript web framework",
  "version": "2.3.3",
  "author": "RVOHealth",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rvohealth/psychic.git"
  },
  "license": "MIT",
  "main": "./dist/cjs/src/package-exports/index.js",
  "module": "./dist/esm/src/package-exports/index.js",
  "types": "./dist/types/src/package-exports/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/package-exports/index.d.ts",
      "import": "./dist/esm/src/package-exports/index.js",
      "require": "./dist/cjs/src/package-exports/index.js"
    },
    "./errors": {
      "types": "./dist/types/src/package-exports/errors.d.ts",
      "import": "./dist/esm/src/package-exports/errors.js",
      "require": "./dist/cjs/src/package-exports/errors.js"
    },
    "./system": {
      "types": "./dist/types/src/package-exports/system.d.ts",
      "import": "./dist/esm/src/package-exports/system.js",
      "require": "./dist/cjs/src/package-exports/system.js"
    },
    "./openapi": {
      "types": "./dist/types/src/package-exports/openapi.d.ts",
      "import": "./dist/esm/src/package-exports/openapi.js",
      "require": "./dist/cjs/src/package-exports/openapi.js"
    },
    "./types": {
      "types": "./dist/types/src/package-exports/types.d.ts",
      "import": "./dist/esm/src/package-exports/types.js",
      "require": "./dist/cjs/src/package-exports/types.js"
    },
    "./utils": {
      "types": "./dist/types/src/package-exports/utils.d.ts",
      "import": "./dist/esm/src/package-exports/utils.js",
      "require": "./dist/cjs/src/package-exports/utils.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "client": "cd client && pnpm start",
    "client:fspec": "cd client && VITE_PSYCHIC_ENV=test BROWSER=none pnpm start",
    "psy": "NODE_ENV=${NODE_ENV:-test} pnpm psyts",
    "psyjs": "node ./dist/test-app/src/cli/index.js",
    "psyts": "NODE_ENV=${NODE_ENV:-test} tsx ./test-app/src/cli/index.ts",
    "gpsy": "tsx ./global-cli/main.ts",
    "build": "echo \"building cjs...\" && rm -rf dist && npx tsc -p ./tsconfig.cjs.build.json && echo \"building esm...\" && npx tsc -p ./tsconfig.esm.build.json",
    "build:test-app": "rm -rf dist && echo \"building test app to esm...\" && npx tsc -p ./tsconfig.esm.build.test-app.json && echo \"building test app to cjs...\" && npx tsc -p ./tsconfig.cjs.build.test-app.json",
    "types:esm:trace": "rm -rf dist && npx tsc -p ./tsconfig.esm.build.json --generateTrace ./typetrace --diagnostics && pnpm analyze-trace ./typetrace --skipMillis 100 --forceMillis 300",
    "dev": "nodemon --quiet --no-stdin",
    "console": "tsx ./test-app/src/conf/repl.ts",
    "uspec": "vitest --config ./spec/unit/vite.config.ts",
    "fspec": "vitest run --config=./spec/features/vite.config.ts",
    "format": "pnpm prettier . --write",
    "lint": "pnpm eslint --no-warn-ignored \"src/**/*.ts\" \"spec/**/*.ts\" \"test-app/**/*.ts\" && pnpm prettier . --check",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "body-parser": "^2.2.1",
    "commander": "^12.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "pluralize-esm": "^9.0.5",
    "yoctocolors": "^2.1.1"
  },
  "peerDependencies": {
    "@rvoh/dream": "^2.2.0",
    "@types/express": "^5.0.1",
    "commander": "^12.1.0",
    "express": "^5.2.1",
    "openapi-typescript": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@jest-mock/express": "^3.0.0",
    "@rvoh/dream": "^2.2.3",
    "@rvoh/dream-spec-helpers": "^2.0.0",
    "@rvoh/psychic-spec-helpers": "^2.0.0",
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/node": "^22.17.1",
    "@types/passport": "^0",
    "@types/passport-local": "^1",
    "@types/pg": "^8.11.8",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/parser": "^8.48.1",
    "@typescript/analyze-trace": "^0.10.1",
    "eslint": "^9.39.1",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "jsdom": "^26.1.0",
    "kysely": "^0.28.5",
    "kysely-codegen": "~0.19.0",
    "luxon-jest-matchers": "^0.1.14",
    "nodemon": "^3.1.11",
    "openapi-typescript": "^7.8.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.12.0",
    "prettier": "^3.3.3",
    "puppeteer": "^24.22.3",
    "supertest": "^7.1.4",
    "tslib": "^2.7.0",
    "tsx": "^4.19.3",
    "typedoc": "^0.26.6",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.9",
    "winston": "^3.14.2"
  },
  "packageManager": "pnpm@10.26.0+sha512.3b3f6c725ebe712506c0ab1ad4133cf86b1f4b687effce62a9b38b4d72e3954242e643190fc51fa1642949c735f403debd44f5cb0edd657abe63a8b6a7e1e402",
  "pnpm": {
    "overrides": {
      "diff": ">=8.0.3"
    }
  }
}
