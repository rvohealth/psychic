{
  "name": "@rvohealth/psychic",
  "description": "Typescript web framework",
  "version": "0.9.1",
  "main": "dist/src/index.js",
  "author": "RVOHealth",
  "repository": "https://github.com/rvohealth/psychic.git",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "publish": "yarn npm publish",
    "psy": "yarn psyts",
    "psyjs": "node ./dist/test-app/src/cli/index.js",
    "psyts": "npx ts-node --transpile-only -r ./src/helpers/loadEnv.ts test-app/src/cli/index.ts",
    "psycore": "PSYCHIC_CORE_DEVELOPMENT=1 yarn psyts",
    "gpsycore": "PSYCHIC_CORE_DEVELOPMENT=1 npx ts-node --transpile-only ./global-cli/main.ts",
    "build": "rm -rf dist && echo \"building psychic app...\" && npx tsc -p ./tsconfig.build.json",
    "build:core": "rm -rf dist && echo \"building psychic core...\" && npx tsc -p ./tsconfig.build-core.json",
    "sync": "NODE_ENV=development yarn --cwd=node_modules/@rvohealth/dream build-boot && NODE_ENV=development yarn --cwd=node_modules/@rvohealth/dream build",
    "syncb": "NODE_ENV=development yarn --cwd=node_modules/@rvohealth/dream build-boot && NODE_ENV=development yarn dream sync:config-cache && yarn --cwd=node_modules/@rvohealth/dream build && yarn build",
    "dev": "PSYCHIC_CORE_DEVELOPMENT=1 NODE_ENV=development ts-node ./test-app/main.ts",
    "console": "PSYCHIC_CORE_DEVELOPMENT=1 npx ts-node --transpile-only --project ./tsconfig.json ./test-app/src/conf/repl.ts",
    "uspec:slow": "PSYCHIC_CORE_DEVELOPMENT=1 yarn psyts sync:all --core && PSYCHIC_CORE_DEVELOPMENT=1 jest --config=./spec/unit/jest.config.js --runInBand --forceExit",
    "uspec": "PSYCHIC_CORE_DEVELOPMENT=1 jest --config=./spec/unit/jest.config.js --runInBand --forceExit",
    "fspec:slow": "PSYCHIC_CORE_DEVELOPMENT=1 yarn psyts sync:all --core && PSYCHIC_CORE_DEVELOPMENT=1 jest --config=./spec/features/jest.config.js --runInBand --forceExit",
    "fspec": "PSYCHIC_CORE_DEVELOPMENT=1 SPEC_SERVER_PORT=7770 jest --config=./spec/features/jest.config.js --runInBand --forceExit",
    "spec-server": "FEATURE_SPEC_RUN=1 SPEC_SERVER_PORT=7770 PORT=7770 npx ts-node --transpile-only ./test-app/spec-server.ts",
    "format": "yarn run prettier . --write",
    "lint": "yarn run eslint --no-warn-ignored \"src/**/*.ts\" \"spec/**/*.ts\" \"test-app/**/*.ts\" && yarn run prettier . --check",
    "prepack": "yarn build",
    "dev-worker": "NODE_ENV=development WORKER_COUNT=1 ts-node --transpile-only ./test-app/worker.ts"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/lodash.groupby": "^4.6.9",
    "@types/pluralize": "^0.0.33",
    "colorette": "^2.0.20",
    "commander": "^12.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express-openapi-validator": "^5.3.7",
    "iso-datestring-validator": "^2.2.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "openapi-typescript": "^7.3.3",
    "pluralize": "^8.0.0"
  },
  "peerDependencies": {
    "@rvohealth/dream": "*",
    "@socket.io/redis-adapter": "*",
    "@socket.io/redis-emitter": "*",
    "@types/express": "*",
    "bullmq": "*",
    "commander": "*",
    "express": "*",
    "ioredis": "*",
    "socket.io": "*",
    "socket.io-adapter": "*"
  },
  "devDependencies": {
    "@eslint/js": "=9.0.0",
    "@jest-mock/express": "^2.1.0",
    "@rvohealth/dream": "=0.13.9",
    "@rvohealth/dream-spec-helpers": "^0.0.8",
    "@rvohealth/psychic-spec-helpers": "^0.0.3",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jest-plugin-context": "^2.9.7",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/lodash.sortby": "^4.7.9",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.5.1",
    "@types/pg": "^8.11.8",
    "@types/supertest": "^6.0.2",
    "bullmq": "^5.12.12",
    "eslint": "^9.9.1",
    "express": "^4.21.1",
    "ioredis": "^5.4.1",
    "jest": "^29.7.0",
    "jest-plugin-context": "^2.9.0",
    "kysely": "^0.27.4",
    "kysely-codegen": "~0.16.4",
    "lodash.sortby": "^4.7.0",
    "luxon": "^3.5.0",
    "pg": "^8.12.0",
    "prettier": "^3.3.3",
    "socket.io": "^4.8.0",
    "socket.io-adapter": "^2.5.5",
    "socket.io-client": "^4.7.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typedoc": "^0.26.6",
    "typescript": "^5.5.4",
    "typescript-eslint": "=7.18.0",
    "winston": "^3.14.2"
  },
  "packageManager": "yarn@4.4.1"
}
