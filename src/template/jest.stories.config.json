{
  "testRegex": "((\\.)(spec))\\.js?$",
  "testEnvironment": "jest-environment-jsdom",
  "setupFiles": [
    "jest-plugin-context/setup",
    "spec/stories/spec-imports.js"
  ],
  "setupFilesAfterEnv": ["./spec/stories/spec-hooks.js"],
  "resetMocks": true,
  "transform": {
    "^.+\\.(js|jsx)$": "babel-jest"
  }
}
