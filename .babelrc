{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-syntax-jsx",
    "require-context-hook",
    [
      "module-resolver",
      {
        "root": ["./"],
        "extensions": [".js"]
      }
    ]
  ],
  "sourceMaps": "inline"
}
