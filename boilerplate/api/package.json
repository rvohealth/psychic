{
  "name": "tbd",
  "version": "0.0.0",
  "main": "src/index.ts",
  "author": "tbd",
  "private": true,
  "scripts": {
    "client": "yarn --cwd=../client start",
    "psy": "npx ts-node --transpile-only src/cli/index.ts",
    "psyjs": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version yarn --cwd=node_modules/@rvohealth/psychic psyjs",
    "build": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version echo \"building $npm_package_name...\" && rm -rf dist && echo \"node --version\" && node --version && echo \"npx tsc --version\" && npx tsc --version && npx tsc -p ./tsconfig.build.json",
    "dev": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=development WORKER_COUNT=0 ts-node --transpile-only ./src/main.ts",
    "dev-worker": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=development WORKER_COUNT=1 ts-node --transpile-only ./src/worker.ts",
    "prod": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=production node ./dist/src/main.js",
    "worker": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=production node ./dist/src/worker.js",
    "sync": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version yarn psy sync:all",
    "uspec": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version jest -w 1 --config=./spec/unit/jest.config.js --forceExit --verbose --logHeapUsage",
    "fspec": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version jest --clearCache && jest --config=./spec/features/jest.config.js --runInBand --forceExit --verbose --logHeapUsage",
    "console": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version npx ts-node --transpile-only --project ./tsconfig.json ./src/conf/repl.ts",
    "format": "yarn run prettier . --write",
    "lint": "yarn run eslint --no-warn-ignored \"src/**/*.ts\" \"spec/**/*.ts\" && yarn run prettier . --check"
  },
  "dependencies": {
    "@bull-board/express": "^5.16.0",
    "@rvohealth/dream": "^0.10.5",
    "@rvohealth/psychic": "^0.7.5",
    "bullmq": "^5.1.11",
    "express-winston": "^4.2.0",
    "express": "^4.19.2",
    "express-openapi-validator": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "kysely-codegen": "^0.11.0",
    "kysely": "^0.27.2",
    "luxon": "^3.4.4",
    "pg": "^8.11.3",
    "redis": "^4.6.13",
    "socket.io": "^4.7.5",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@jest-mock/express": "^2.0.2",
    "@playwright/test": "^1.41.1",
    "@rvohealth/dream-spec-helpers": "^0.0.2",
    "@rvohealth/psychic-spec-helpers": "^0.0.2",
    "@types/express": "^4.17.21",
    "@types/jest-plugin-context": "^2.9.7",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.17",
    "@types/pg": "^8.11.3",
    "@types/supertest": "^6.0.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^9.0.0",
    "expect-playwright": "^0.8.0",
    "jest-dev-server": "^9.0.2",
    "jest-playwright-preset": "^4.0.0",
    "jest": "^29.7.0",
    "luxon-jest-matchers": "^0.1.14",
    "playwright-chromium": "^1.41.2",
    "playwright-firefox": "^1.41.2",
    "playwright": "^1.41.2",
    "prettier": "^3.2.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^7.6.0",
    "typescript": "^5.4.5"
  }
}
